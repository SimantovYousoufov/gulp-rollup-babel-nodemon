{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb/base",
  "ecmaFeatures": {
    "modules": true
  },
  "rules" : {
    "array-bracket-spacing": 0,
    "indent" : [0, 2],
    "no-multi-spaces":0,
    "new-cap": 0,
    "no-undef" : 1,
    "vars-on-top" : 0,
    "no-unused-vars": 1,
    "no-use-before-define": 0,
    "comma-dangle" : 0
  }
}